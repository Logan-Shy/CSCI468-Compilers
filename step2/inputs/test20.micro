PROGRAM fibonacci
BEGIN

	STRING input := "Please input an integer number: ";
	STRING space := " ";
	STRING eol := "\n";

	FUNCTION INT F (INT n)
	BEGIN
		IF (n = 0)
			RETURN 0;
		ENDIF
		IF (n > 2)
			RETURN F(n-1)+F(n-2);
		ELSE
			RETURN 1;
		ENDIF

	END
	
	
	FUNCTION VOID main ()
	BEGIN
		INT i, end, result;
		WRITE(input);
		READ(end);

	i := 0;
	WHILE (i != end)
		result := F(i);
		WRITE (i,space);
		WRITE (result,eol);
		i := i + 1;	
	ENDWHILE

	END

END	
